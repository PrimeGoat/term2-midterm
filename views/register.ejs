<%- include('../partials/header', {title: "Test"}); %>

<div class="mainCol">
	<div class="output loginBox">
		<h1>REGISTRATION</h1>
		<h3>
			<% if (success.length) { %>
				<%= success %>
			<% } %>
			<% if (errors.length) { %>
				<%= errors %>
			<% } %><br>
		</h3>
		<form action="/api/v1/register" method="post">
			<div class=formEntries>
				<label for="name">Your Name:</label><br>
				<input type="text" name="name" id="name"><br><br>
				<label for="email">Email Address:</label><br>
				<input type="text" name="email" id="email"><br><br>
				Your password will be emailed to you.<br><br>
				<input type="submit" name="submit" value="Submit">
			</div>
		</form>


	</div>
</div>

<%- include('../partials/footer', {title: "Test"}); %>
